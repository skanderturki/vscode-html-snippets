{
  "HTML5 Start": {
    "prefix": ["html5-start"],
    "body": ["<!DOCTYPE html>",
            "<html lang=\"en\">",
            "<head>",
            "  <!-- general meta tags -->",
            "  <meta charset=\"utf-8\">",
            "  <meta name=\"author\" content=\"$1\">",
            "  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">",
            "  <!-- site specific tags -->",
            "  <title>$2</title>",
            "  <!-- style sheets -->",
            "  <link rel=\"stylesheet\" href=\"css-reset.css\">",
            "  <link rel=\"stylesheet\" href=\"$3.css\">",
            "  <!-- javascript files -->",
            "  <script src=\"$4.js\"></script>",
            "</head>",
            "<body>",
            "  <header>",
            "    $5",
            "    <nav>",
            "      $6",
            "    </nav>",
            "  </header>",
            "  <main>",
            "    <section>",
            "      <article>",
            "        $7",
            "      </article>",
            "    </section>",
            "  </main>",
            "  <footer>",
            "    $8",
            "  </footer>",
            "</body>",
            "</html>"],
    "description": "An HTML5 skeleton."
  },
  "HTML Table": {
    "prefix": ["html-table"],
    "body": ["<table>",
            "    <tr>",
            "      <th>$1",
            "      <th>$2",
            "      <th>$3",
            "    <tr>",
            "      <td>$4",
            "      <td>$5",
            "      <td>$6",
            "    <tr>",
            "      <td>$7",
            "      <td>$8",
            "      <td>$9",
            "</table>"],
    "description": "A HTML Table skeleton."
  },
  "HTML Form": {
    "prefix": ["html-form"],
    "body": ["<form method=\"${1|post,get|}\" action=\"$2\">",
            "  <fieldset>",
            "  <legend>$3</legend>",
            "    <div>",
            "      <label>$4</label>",
            "      <input type=\"${5|text,number,email,date,password,url|}\" name=\"$6\">",
            "    </div>",     
            "    ",
            "    <input type=\"${7|submit,button|}\">",
            "  </fieldset>",
            "</form>"],
    "description": "A HTML Form skeleton."
  },
  "HTML5 Start - optimized": {
    "prefix": ["html5-start-optimized"],
