{
  "HTML5 Start": {
    "prefix": ["html5-start"],
    "body": ["<!DOCTYPE html>",
            "<html lang=\"en\">",
            "<head>",
            "  <!-- general meta tags -->",
            "  <meta charset=\"utf-8\">",
            "  <meta http-equiv='X-UA-Compatible' content='IE=edge'>",
            "  <meta name='viewport' content='width=device-width, initial-scale=1'>",
            "  <!-- site specific tags -->",
            "  <title>$1</title>",
            "  <link rel='stylesheet' type='text/css' media='screen' href='css-reset.css'>",
            "  <!-- document styles -->",
            "  <script>",
            "  $2",
            "  </script>",
            "</head>",
            "<body>",
            "  <header>",
            "  $3",
            "  </header>",
            "  <main>",
            "  $4",
            "  </main>",
            "  <footer>",
            "  $5",
            "  </footer>",
            "</body>",
            "</html>"],
    "description": "An HTML5 skeleton."
  },
  "HTML Table": {
    "prefix": ["html-table"],
    "body": ["<table>",
            "  <!-- table header -->",
            "  <tr>",
            "    <th>$1</th><th>$2</th><th>$3</th>",
            "  </tr>",
            "  <!-- table data -->",
            "  <tr>",
            "    <td>$4</td><td>$5</td><td>$6</td>",
            "  </tr>",
            "  <tr>",
            "    <td>$7</td><td>$8</td><td>$9</td>",
            "  </tr>",
            "</table>"],
    "description": "A HTML Table skeleton."
  },
  "HTML Form": {
    "prefix": ["html-form"],
    "body": ["<form method=\"$1\" action=\"$2\">",
            "  <fieldset>",
            "  <legend>$3</legend>",
            "    <p>",
            "      <label>$4</label>",
            "      <input type=\"$5\" name=\"$6\"/>",
            "    </p>",     
            "    ",
            "    <input type=\"$7\" />",
            "  </fileldset>",
            "</form>"],
    "description": "A HTML Form skeleton."
  }
}
