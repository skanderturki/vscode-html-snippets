            "  $5",
            "  <nav>",
            "    $6",
            "  </nav>",
            "</header>",
            "<main>",
            "  <section>",
            "    <article>",
            "      $7",
            "    </article>",
            "  </section>",
            "</main>",
            "<footer>",
